{
  "name": "ai-app-vue-toss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:live": "cross-env BASE_URL=ghpageslive npm run build && cross-env BASE_URL=ghpageslive node ghpages.js",
    "build:prod": "cross-env BASE_URL=ghpagesprod npm run build && cross-env BASE_URL=ghpagesprod node ghpages.js",
    "build:test": "cross-env BASE_URL=ghpagestest npm run build && cross-env BASE_URL=ghpagestest node ghpages.js",
    "build:dev": "cross-env BASE_URL=ghpagesdev npm run build && cross-env BASE_URL=ghpagesdev node ghpages.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --fix --ext .js,.vue src",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "ali-oss": "^6.6.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.22",
    "element-ui": "^2.13.0",
    "file-saver": "^2.0.2",
    "js-cookie": "^2.2.1",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.1.0",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "gh-pages": "^2.2.0",
    "html2canvas": "^1.0.0-rc.5",
    "lerna": "^3.20.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
